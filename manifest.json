{
  "name": "Music Controller",
  "version": "0.1.0",
  "description": "Creates keyboard shortcuts to control music.google.com",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs", "*://play.google.com/music/*"
  ],
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"]
    }
  ]
}
